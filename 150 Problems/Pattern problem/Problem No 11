/*
E D C B A
  D C B A
    C B A
      B A
        A
*/

import java.io.*;
import java.util.*;

class Main {
    public static void main(String[] args) {
        int n=5;
        for(int i=1;i<=n;i++){
            for(int j=1;j<i;j++){
                System.out.print(" ");
            }
            for(char ch=(char)('E'-i+1);ch>='A';ch--){
                System.out.print(ch);
            }
            System.out.println(" ");
        }
    }
}
