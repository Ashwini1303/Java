/*
1
00
111
0000
11111
*/


import java.io.*;

class Main {
    public static void main(String[] args) {
        int N=5;
        for(int i=0;i<=N;i++){
            int digit =1;
            if(i%2==0){
                digit =0;
            }
            for(int j=1;j<=i;j++){
                System.out.print(digit);
            }
        System.out.println("");
        }
    }
}
