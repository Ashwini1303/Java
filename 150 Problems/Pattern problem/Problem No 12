/*
Print braces pattern program
int n=2
Output:
{ }
{ } {{ }}

*/

import java.io.*;
import java.util.*;

class Main {
    public static void main(String[] args) {
        int n = 2;
        for (int i=1; i<=n; i++) {
            for (int j=1; j<=i; j++) {
                String s = " ";
                for (int k=1; k<=j; k++) {
                    s = "{" + s + "}";
                }
                System.out.print(s + " ");
            }
            System.out.println();
        }
    }
}
