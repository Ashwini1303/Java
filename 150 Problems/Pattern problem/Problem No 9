*/

import java.io.*;
import java.util.*;

class Main {
    public static void main(String[] args) {
        int n=5;
        for(int i=1;i<=n;i++){
            for(int j=1+n-i;j>=1;j--){
                System.out.print(j);
            }
            System.out.println("");
        }
        
    }
}
